    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run Console App",
            "program": "${workspaceFolder}/dist/ConsoleApp.js",
            "preLaunchTask": "tsc: build - tsconfig.dev.json",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "env": {
                "PROFILE_API": "1"
            }
        }
    ]
}